  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.9",
    "drizzle-zod": "^0.5.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "svelte": "^5.0.0-next.1",
    "svelte-adapter-bun": "^0.5.1",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
